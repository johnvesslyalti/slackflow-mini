generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Request {
  id              String   @id @default(uuid())
  slackMessageId  String?
  channelId       String?
  createdBy       String?
  status          String
  owner           String?
  externalTicketId String?
  createdAt       DateTime @default(now())
  resolvedAt      DateTime?
}